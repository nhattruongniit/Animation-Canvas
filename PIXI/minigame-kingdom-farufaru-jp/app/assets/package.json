{
  "name": "assets",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all sprites image-min webpack",
    "webpack": "cross-env NODE_ENV=production webpack",
    "dev": "webpack-dev-server",
    "sounds": "node ./node_modules/.bin/audiosprite --format howler --silence 1 --path /static/build/sounds --output ../public/static/build/sounds/sprite --loop ./sounds/bgm.wav ./sounds/*.wav",
    "sprites": "npm-run-all sprites:*",
    "sprites:html": "spritesmith --rc .spritesmith.html.js",
    "sprites:canvas": "spritesmith --rc .spritesmith.canvas.js",
    "image-min": "npm-run-all image-min:*",
    "image-min:sprites-html": "find ../public/static/build/sprites-html -name '*.png' | imageoptim --no-color --image-alpha --min-quality 50",
    "image-min:sprites-canvas": "find ../public/static/build/sprites-canvas -name '*.png' | imageoptim --no-color --image-alpha --min-quality 50",
    "image-min:static-image": "find -E ../public/static/images -regex '^.+\\.(jpg|png)$' | imageoptim --jpeg-mini --no-color --image-alpha --min-quality 50",
    "lint": "eslint --ext js,vue --config .eslintrc ./"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "audiosprite": "^0.6.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.3.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "glob": "^7.1.2",
    "imageoptim-cli": "^1.14.9",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^6.0.6",
    "spritesmith-cli": "^1.1.2",
    "spritesmith-texturepacker": "^1.0.0",
    "vue-loader": "^12.2.1",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^16.7.0",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "compare-versions": "^3.0.1",
    "howler": "^2.0.4",
    "lodash": "^4.17.4",
    "number-to-locale-string": "^1.0.1",
    "pixi.js": "^4.5.3",
    "platform": "^1.3.4",
    "store": "^2.0.12",
    "vue": "^2.3.4",
    "vue-router": "^2.6.0"
  }
}
